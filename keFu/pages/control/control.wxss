.kt_btn{
  position: absolute;
  left: 50%;
  margin-left: -28rpx;
  top:68rpx;
  z-index: 3;
  width:56rpx;
  height:56rpx;
  border-radius: 28rpx;
  background:linear-gradient(180deg,rgba(114,190,255,1) 0%,rgba(54,164,255,1) 100%);
  transition: top 0.4s;
}
.kt_kg_lm .kt_btn_off{
  top:5rpx;
  background:linear-gradient(180deg,rgba(229,229,229,1) 0%,rgba(190,190,190,1) 100%);
}
.kt_wendu image{
  width: 392rpx;
  height: 268rpx;
