/* pages/select/selectPages.wxss */
.pageUl{
  height: 120rpx;
  width: 95%;
  border-radius:8rpx;
  margin: 20rpx auto;
  box-shadow: 0 0 9rpx rgb(219, 219, 219);
  line-height: 120rpx;
  box-sizing: border-box;
  overflow: hidden;
  background-color: #FFF
}
.pageUl .ty{
  display: inline-block;
 
  font-size: 20rpx;
}
.pageUl .image1{
  width: 100rpx;
  height: 100rpx;
  margin: 10rpx
}
.pageUl .image2{
  width: 40rpx;
  height: 40rpx;
  margin: 40rpx 10rpx 40rpx;
  float: right;
}
.pageUl .text{
  height: 80rpx;
  line-height: 40rpx;
  box-sizing: border-box;
  overflow: hidden;
  margin-bottom: 20rpx;
  margin-left: 20rpx
}
/* 统一按钮 */
.btn{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  background-color: #0984e3;
  color: #FFF;
  line-height: 90rpx;
  font-size: 30rpx;
  text-align: center
}